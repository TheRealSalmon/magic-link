{% extends "base" %}
{% block content %}
<h1>Sign Up</h1>

<form action="/register-email" method="POST">
  <div class="mb-3">
    <label class="form-label">Email Address</label>
    <input type="text" name="email" class="form-control">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>

{% endblock content %}